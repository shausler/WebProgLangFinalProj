<h1>New Part</h1>
  <%= form_for :part, url: part_path do |f| %>
  <p>
    <%= f.label :serialNumber %><br>
    <%= f.text_field :serialNumber %>
  </p>

  <p>
    <%= f.label :name %><br>
    <%= f.text_area :name %>
  </p>

  <p>
    <%= f.label :partType %><br>
    <%= f.text_area :partType %>
  </p>

  <p>
    <%= f.label :vehicleID %><br>
    <%= f.text_area :vehicleID %>
  </p>

  <p>
    <%= f.label :weight %><br>
    <%= f.text_area :weight %>
  </p>

  <p>
    <%= f.label :shippingCost %><br>
    <%= f.text_area :shippingCost %>
  </p>

  <p>
    <%= f.label :dimensions %><br>
    <%= f.text_area :dimensions %>
  </p>

  <p>
    <%= f.label :price %><br>
    <%= f.text_area : %>
  </p>
    
  <p>
    <%= f.submit %>
  </p>
<% end %>